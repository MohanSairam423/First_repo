pipeline {
   agent any
   tools {
        maven 'Maven'
    }
    
    stage('Build') 
    {

		stage('dev') {

    	 when {branch 'dev'}

    	 	steps {
				echo 'Deploying in dev branch....'
				snDevOpsStep ()
				snDevOpsChange()
	 		}

  		}

		  stage('master') {
		
		    when {branch 'master'}
		    steps {
				echo 'Deploying in master branch....'
				snDevOpsStep ()
				snDevOpsChange()
			}
		
		  }

	}
   
}
